{% set title = title|default('(っ◕‿◕)っ Core') %}
{% extends 'special/html5.twig' %}

{% block head %}
  <style>
  body{background:#DAD9EB;text-align:center;user-select:none;-webkit-user-select:none;-moz-user-select:none;cursor:default}body:after{content:"";background-image:url(data:image/svg+xml;base64,);opacity:.020;top:0;left:0;bottom:0;right:0;position:absolute;z-index:-1}#container{top:25%;left:-999999px;right:-999999px;position:fixed;}#logo{animation:bounce cubic-bezier(0.19,1,.22,1) 1.5s alternate infinite;-webkit-animation:bounce cubic-bezier(0.19,1,.22,1) 1.5s alternate infinite}@keyframes bounce{to{transform:translateY(-12px) scale(1.03)}}@-webkit-keyframes bounce{to{-webkit-transform:translateY(-12px) scale(1.03);}}#text{margin-top:7px}h1,p{margin:0;padding:0;font-family:Helvetica,sans-serif;font-weight:100;font-style:normal;font-size: 40px;margin-top: 1em;}h1{color:rgba(0,0,0,.75);}p{color:rgba(0,0,0,.25);text-shadow:0 1px 0 rgba(255,255,255,.75)}.shadowed {-webkit-filter: drop-shadow(12px 12px 7px rgba(0,0,0,0.5));}
  </style>
{% endblock %}

{% block body %}
  <div id="container">
  <img width="312" height="327" id="logo" class="shadowed" src="data:image/png;base64," />
    <div id="text">
      <h1>Now you can start.</h1>
    </div>
  </div>
{% endblock %}
  